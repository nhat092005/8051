ORG 0000H
	JMP MAIN
	
ORG 001BH
	JMP TIMER1_ISR
	
ORG 0030H
MAIN:
	SETB P2.0
	MOV R7, #100
	
	SETB ET1
	SETB EA
	
	MOV TMOD, #10H
	MOV TH1, #8AH
	MOV TL1, #0D0H
	
	SETB TR1

LOOP:
	JMP LOOP
	
TIMER1_ISR:
	MOV TH1, #8AH
	MOV TL1, #0D0H
	DJNZ R7, EXIT_ISR
	MOV R7, #100
	CPL P2.0
	
EXIT_ISR:
	RETI

END
