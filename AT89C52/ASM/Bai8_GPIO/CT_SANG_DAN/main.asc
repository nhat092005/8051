ORG 0000H

BEGIN_ENTRY:
	CLR C
	MOV A, #11111111B
	
LOOP:
	MOV P2, A
	RLC A
	CALL DELAY200ms
	JC LED_ON
	JNC LED_OFF
	JMP LOOP

LED_ON:
	CLR C
	JMP LOOP
	
LED_OFF:
	CLR C
	MOV A, #11111111B
	JMP LOOP
	
DELAY200ms:
	MOV R2, #200
LAP2:
	MOV R1, #200
LAP1:
	NOP
	NOP
	NOP
	DJNZ R1, LAP1
	DJNZ R2, LAP2
	RET
	
END
